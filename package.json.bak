{
  "name": "web-react",
  "description": "Another Webpack/React/Flux starter kit.",
  "version": "0.0.7",
  "homepage": "https://github.com/darul75/web-react",
  "repository": {
    "type": "git",
    "url": "https://github.com/darul75/web-react.git"
  },
  "scripts": {
    "dev": "webpack --config conf/webpack-dev.js | webpack-dev-server --port 8080 --config conf/webpack-dev.js --hot --history-api-fallback --progress --colors --inline --content-base ./build",
    "dev-server-client": "webpack --config conf/webpack-dev-server-client.js | webpack-dev-server --port 8081 --config conf/webpack-dev-server-client.js --hot --history-api-fallback --progress --colors --inline --content-base ./build",
    "dev-server": "webpack --watch --progress --config conf/webpack-dev-server.js | node build/server.js",
    "dev-server-debug": "webpack --watch --progress --config conf/webpack-dev-server.js | node-debug --debug-brk build/server.js --debug --source-maps",
    "build": "webpack --config conf/webpack-prod.js --progress --profile --colors",
    "start": "node dist/server.js",
    "test": "./node_modules/.bin/mocha app/**/*-test.js --colors --compilers js:babel/register --recursive"
  },
  "devDependencies": {    
    "babel-core": "^5.1.11",
    "babel-eslint": "^3.1.1",
    "babel-loader": "5.0.0",
    "babel-runtime": "^5.1.11",
    "clean-webpack-plugin": "^0.1.2",
    "css-loader": "^0.10.1",
    "eslint": "^0.20.0",
    "eslint-loader": "^0.11.2",
    "eslint-plugin-react": "^2.2.0",
    "extract-text-webpack-plugin": "^0.6.0",
    "file-loader": "^0.8.1",
    "html-webpack-plugin": "^1.2.0",
    "jsdom": "^2.0.0",
    "jsx-loader": "^0.13.2",
    "mocha": "^2.1.0",
    "node-sass": "^2.1.1",
    "react-hot-loader": "^1.2.5",
    "react-router": "^0.13.2",
    "sass-loader": "0.4.2",
    "style-loader": "^0.10.2",
    "url-loader": "^0.5.5",
    "webpack": "^1.9.4",
    "webpack-dev-server": "^1.8.2"
  },
  "dependencies": {
    "alt": "^0.15.6",
    "babel": "^5.1.11",
    "classnames": "^2.1.1",
    "express": "^4.12.3",    
    "node-fetch": "^1.2.1",
    "object-assign": "^2.0.0",
    "react": "^0.13.2",    
    "serve-favicon": "^2.2.1",
    "whatwg-fetch": "^0.8.1"
  },
  "keywords": [
    "react",
    "flux",
    "isomorphic",
    "webpack",
    "kit"
  ]
}
