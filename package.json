{
  "name": "web-react",
  "description": "Another Webpack/React/Flux starter kit.",
  "version": "1.1.0",
  "homepage": "https://github.com/darul75/web-react",
  "repository": {
    "type": "git",
    "url": "https://github.com/darul75/web-react.git"
  },
  "scripts": {
    "dev": "webpack --config conf/webpack-dev.js | webpack-dev-server --port 8080 --config conf/webpack-dev.js --hot --history-api-fallback --progress --colors --inline --content-base ./build",
    "dev-server-client": "webpack --progress --watch --config conf/webpack-dev-server.js | webpack-dev-server --port 8081 --config conf/webpack-dev-server.js --hot --history-api-fallback --progress --colors --inline --content-base ./build",
    "dev-server": "node build/server.js",
    "dev-server-debug": "node-debug --debug-brk build/server.js --debug --source-maps",
    "build": "webpack --config conf/webpack-prod.js --progress --profile --colors",
    "start": "node dist/server.js",
    "test": "mocha app/**/*-test.js --colors --compilers js:babel/register --recursive",
    "test-debug": "mocha --debug-brk app/**/*-test.js --colors --compilers js:babel/register --recursive --source-maps"
  },
  "devDependencies": {
    "babel-core": "^5.8.23",
    "babel-eslint": "^4.1.3",
    "babel-loader": "^5.3.2",
    "babel-runtime": "^5.1.11",
    "clean-webpack-plugin": "^0.1.2",
    "css-loader": "^0.10.1",
    "deep-equal": "^1.0.1",
    "eslint": "^0.20.0",
    "eslint-loader": "^0.11.2",
    "eslint-plugin-react": "^2.2.0",
    "extract-text-webpack-plugin": "^0.6.0",
    "file-loader": "^0.8.1",
    "history": "^1.17.0",
    "html-webpack-plugin": "^1.2.0",
    "invariant": "2.1.1",
    "jsdom": "^2.0.0",
    "json-loader": "^0.5.2",
    "jsx-loader": "^0.13.2",
    "mocha": "^2.1.0",
    "node-sass": "^3.1.2",
    "optimist": "^0.6.1",
    "qs": "4.0.0",
    "react-hot-loader": "^1.3.0",
    "sass-loader": "1.0.2",
    "style-loader": "^0.10.2",
    "url-loader": "^0.5.5",
    "warning": "^2.1.0",
    "webpack": "^1.9.10",
    "webpack-dev-middleware": "^1.2.0",
    "webpack-dev-server": "^1.8.2"
  },
  "dependencies": {
    "alt": "^0.17.0",
    "alt-container": "^1.0.2",
    "babel": "^5.1.11",
    "classnames": "^2.1.1",
    "express": "^4.12.3",
    "firmata": "^0.8.0",
    "fs": "0.0.2",
    "immutable": "^3.7.4",
    "iso": "^4.1.0",
    "johnny-five": "^0.9.14",
    "lodash": "^3.10.1",
    "marked": "^0.3.3",
    "node-fetch": "^1.2.1",
    "object-assign": "^2.0.0",
    "react": "^0.14.0",
    "react-dom": "^0.14.0",
    "react-helmet": "^2.1.0",
    "react-router": "latest",
    "repl": "^0.1.3",
    "reqwest": "^2.0.5",
    "serve-favicon": "^2.2.1",
    "socket.io": "^1.3.7",
    "socket.io-client": "^1.3.7",
    "striptags": "^2.1.1",
    "whatwg-fetch": "^0.8.1",
    "xhr2": "^0.1.3"
  },
  "keywords": [
    "react",
    "flux",
    "isomorphic",
    "webpack",
    "kit",
    "express",
    "node"
  ],
  "license": "MIT"
}
